{
	"name": "roave/phpstan-disallow-float",
	"description": "PHPStan Disallow Float Rules",
	"license": [
		"MIT"
	],
	"require": {
		"php": "~7.1",
		"nikic/php-parser": "^4.0",
		"phpstan/phpstan": "^0.10"
	},
	"require-dev": {
		"phing/phing": "^2.16.0",
		"phpstan/phpstan-phpunit": "^0.10",
		"phpstan/phpstan-strict-rules": "^0.10",
		"phpunit/phpunit": "^7.4",
		"doctrine/coding-standard": "^5.0.0",
		"squizlabs/php_codesniffer": "^3.4.0"
	},
	"autoload": {
		"psr-4": {
			"Roave\\PHPStan\\Rules\\Floats\\": "src/"
		}
	},
	"autoload-dev": {
		"classmap": [
			"tests/"
		]
	},
	"minimum-stability": "dev",
	"prefer-stable": true
}
